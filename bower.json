{
  "name": "simple-list",
  "version": "0.0.5",
  "description": "simple-list extends from `Polymer/core-list`, it displays a virtual, infinite list. Instead of native scroll this component use iscroll",
  "keywords": [
    "Polymer",
    "web-components",
    "infinite",
    "list"
  ],
  "authors": [
    "Walter Chacon Solis"
  ],
  "license": "MIT",
  "homepage": "https://github.com/debianw/simple-list",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "components",
    "examples",
    "test",
    "tests"
  ],
  "dependencies": {
    "polymer": "~0.4.2",
    "core-list": "Polymer/core-list#~0.4.2",
    "simple-scroller": "~0.0.2"
  },
  "devDependencies": {
    "font-roboto": "Polymer/font-roboto#~0.4.2",
    "core-header-panel": "Polymer/core-header-panel#~0.4.2",
    "core-toolbar": "Polymer/core-toolbar#~0.4.2"
  }
}
